{
	"presets": [
		"@babel/preset-react"
	],
	"plugins": [
		"@babel/plugin-proposal-class-properties",
		"@babel/plugin-proposal-object-rest-spread",
		["transform-react-remove-prop-types", {
			"mode": "wrap"
		}]
	],
	"env": {
		"cjs": {
			"presets": [
				["@babel/preset-env", {
					"targets": {
						"browsers": [
							"ie 11"
						]
					}
				}]
			]
		},
		"es": {
			"presets": [
				["@babel/preset-env", {
					"modules": false,
					"targets": {
						"browsers": [
							"ie 11"
						]
					}
				}]
			]
		},
		"test": {
			"presets": [
				["@babel/preset-env", {
					"targets": {
						"node": "current"
					}
				}]
			]
		}
	}
}
